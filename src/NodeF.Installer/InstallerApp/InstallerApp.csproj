<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="6.15.0" />
    <PackageReference Include="NBitcoin" Version="6.0.18" />
    <PackageReference Include="SshKeyGenerator" Version="1.1.40" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\NodeF.Installer.Models\NodeF.Installer.Models.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="LoadSavedFiles.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <Page Update="LoadSavedFiles.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>
