{
	"name": "StripePayments",
	"version": "1.0.0",
	"description": "gRPC endpoint for stripe payments",
	"main": "server.js",
	"author": "Andrew Mingst",
	"license": "MIT",
	"scripts": {
		"proto-gen": "./proto-gen.sh",
		"dev-server": "ts-node ./Service/server",
		"dev-client": "ts-node ./Client/client",
		"start-server": "node dist/Service/server.js",
		"start-client": "node dist/Client/client.js",
		"start-all": "concurrently \"npm:start-server\" \"npm:start-client\""
	},
	"devDependencies": {
		"@grpc/grpc-js": "^1.4.5",
		"@grpc/proto-loader": "^0.6.7",
		"ts-node": "^10.4.0",
		"tslib": "^2.3.1",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"gulp-typescript": "^6.0.0-alpha.1"
	}
}
