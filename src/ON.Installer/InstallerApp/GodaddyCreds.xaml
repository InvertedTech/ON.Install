<Window x:Class="InstallerApp.GodaddyCreds"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InstallerApp"
        mc:Ignorable="d"
        Title="GoDaddy Credentials" Height="150" Width="300">
    <StackPanel>
        <TextBlock Text="GoDaddy API Key" />
        <PasswordBox Grid.Column="1" Grid.Row="1" PasswordChar="*" Name="passKey"/>
        <TextBlock Text="GoDaddy API Secret" />
        <PasswordBox Grid.Column="1" Grid.Row="1" PasswordChar="*" Name="passSecret"/>
        <Button Content="OK" Click="OKButton_Click" />
    </StackPanel>
</Window>
